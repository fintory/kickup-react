{
  "projectName": "kickup-react",
  "cleanOnBuild": true,
  "server": {
    "defaultHost": "0.0.0.0",
    "defaultPort": "3000"
  },
  "progressive": {
    "enabled": false,
    "themeColor": "#000000"
  },
  "polyfillIo": {
    "enabled": true,
    "features": ["default"]
  },
  "serviceWorker": {
    "enabled": false
  },
  "defaultMetaTags": {
    "enabled": true,
    "openGraphEnabled": true,

    "title": "kickup-react",
    "description": "kickup-react is a boilerplate to help you get up and started quickly, by providing everything you need for a seamless front-end development",
    "keywords": "kickup-react,boilerplate,front-end,development",
    "robots": "nofollow",
    "type": "Website",
    "siteName": "kickup-react"
  }
}
